<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jackson.springboot.demo.mapper.UserMapper">

    <select id="selectAll" resultType="com.jackson.springboot.demo.entity.User">
        select id, name , create_time AS createTime
        from t_user
    </select>

    <select id="selectById" resultType="com.jackson.springboot.demo.entity.User">
        select id, name , create_time AS createTime
        from t_user where id = #{id}
    </select>

</mapper>